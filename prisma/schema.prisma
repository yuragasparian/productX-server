generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Products {
  id            Int    @id @default(autoincrement())
  name          String
  sku           Int    @unique
  category      String
  description   String
  price         Int
  stockQuantity Int
}
